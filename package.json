{
  "name": "mushi-social",
  "version": "1.0.0",
  "description": "A full-stack social media application",
  "scripts": {
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "deploy:sepolia": "forge script script/Deploy.s.sol:DeployMushiRegistry --rpc-url $ETH_RPC_URL --broadcast",
    "deploy:sepolia:verify": "forge script script/Deploy.s.sol:DeployMushiRegistry --rpc-url $ETH_RPC_URL --broadcast --verify"
  },
  "keywords": ["social", "media", "fullstack"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
