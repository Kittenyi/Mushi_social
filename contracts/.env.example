# Foundry deploy (used by forge script)
# Copy to .env in repo root or set in shell.

# Deployer private key (no 0x prefix, or with 0x both work)
PRIVATE_KEY=your_deployer_private_key_here

# RPC URL for the chain to deploy to
ETH_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_KEY
# Mainnet example: https://eth-mainnet.g.alchemy.com/v2/YOUR_KEY

# Optional: version & app URI for MushiRegistry constructor
MUSHI_VERSION=1.0.0
MUSHI_APP_URI=https://mushi.social

# Optional: Etherscan API key for forge script --verify
ETHERSCAN_API_KEY=your_etherscan_api_key
